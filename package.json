{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.3",
    "@ant-design/pro-layout": "^5.0.7",
    "@umijs/hooks": "^1.8.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.13",
    "antd": "^4.0.4",
    "axios": "^0.21.1",
    "constate": "^2.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-combine-provider": "^0.1.3",
    "react-dom": "^16.12.0",
    "umi": "^3.0.13",
    "umi-request": "^1.2.19",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.2",
    "mockjs": "^1.1.0"
  }
}
